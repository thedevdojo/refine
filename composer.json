{
    "name": "devdojo/refine",
    "description": "A developer-only Chrome extension and Laravel package for live-editing Blade templates directly in the browser",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "DevDojo",
            "email": "hello@devdojo.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "DevDojo\\Refine\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DevDojo\\Refine\\RefineServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
